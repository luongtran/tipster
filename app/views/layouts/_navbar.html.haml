.navbar.navbar-default
  .navbar-inner
    %a.brand{href: root_path} HOME
    %ul.nav
      %li
        %a{href: "#"} Link
    %ul.nav.pull-right
      - if current_user
        %li
          = link_to "Hi #{current_user.name}, Sign Out", destroy_user_session_path
      - else
        %li
          = link_to 'Register', new_user_registration_path
        %li
          = link_to 'Sign In', new_user_session_path